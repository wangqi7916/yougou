.main .swiper {
  width: 100%;
}
.main .swiper image {
  width: 100%;
}
.main .nav {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx 0;
  background: #fff;
}
.main .nav .nav_item {
  flex: 1;
  text-align: center;
}
.main .nav .nav_item image {
  width: 80%;
}
.main .floor {
  width: 100%;
}
.main .floor .floorTitle {
  width: 100%;
  background: #f1f3f5;
  padding: 10rpx 0 0 10rpx;
}
.main .floor .floor_content {
  padding: 20rpx;
  background: #fff;
  display: flex;
}
.main .floor .floor_content .floor_content_left {
  width: 233rpx;
  flex: 0 0 233rpx;
}
.main .floor .floor_content .floor_content_left image {
  width: 233rpx;
  height: 386rpx;
}
.main .floor .floor_content .floor_content_right {
  flex: 1;
  width: 100%;
}
.main .floor .floor_content .floor_content_right image {
  width: 233rpx;
  height: 188rpx;
  display: block;
  margin-left: 6rpx;
  margin-top: 6rpx;
  float: left;
}
